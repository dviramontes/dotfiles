[user]
	name = David Viramontes
	email = dviramontes@gmail.com
[color]
	ui = true
[core]
	editor = vim
	excludesfile = /Users/dv/.gitignore
[alias]
	co = checkout
	cm = commit
	s  = status
	b  = branch
	type = cat-file -t
	dump = cat-file -p
	up = push origin HEAD
	bcd = branch --sort=-committerdate
	bca = branch --sort=committerdate
	cp = cherry-pick
	can = commit --amend --no-edit
[init]
	defaultBranch = main
[credential "https://github.com"]
	helper =
	helper = !/opt/homebrew/bin/gh auth git-credential
[credential "https://gist.github.com"]
	helper =
	helper = !/opt/homebrew/bin/gh auth git-credential
